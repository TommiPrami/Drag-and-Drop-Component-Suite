  </MACROS>
  <OPTIONS>
    <IDLCFLAGS value="-I..\..\components -I$(BCB)\Imports -I$(BCB)\include -I$(BCB)\include\vcl 
      -src_suffixcpp -DNO_WIN32_LEAN_AND_MEAN -boa"/>
